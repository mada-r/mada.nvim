{
    "Lua.runtime.version": "Lua 5.4",
    "Lua.workspace.ignoreDir": [
        "server/meta",
        "server/locale",
        "locale",
    ],
    "Lua.diagnostics.globals": [
        "ERR",
        "OUT",
        "IN",
        "log",
        "ac",
        "_G",
        "GC",
        "ID",
        "DEVELOP",
        "DBGPORT",
        "DBGWAIT",
        "TEST"
